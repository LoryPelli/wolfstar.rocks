generator client {
  provider     = "prisma-client"
  output       = "../../server/database/generated"
  moduleFormat = "esm"
  previewFeatures = ["driverAdapters", "multiSchema", "queryCompiler"]
}

generator json {
  provider = "prisma-json-types-generator"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
  schemas = ["user", "schedule", "moderation", "migration", "guild"]
}




{
	"name": "@wolfstar-project/dashboard",
	"version": "1.0.0",
	"private": true,
	"bugs": {
		"url": "https://github.com/wolfstar-project/wolfstar.rocks/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wolfstar-project/wolfstar.rocks.git"
	},
	"license": "Apache-2.0",
	"type": "module",
	"scripts": {
		"build": "pnpm run pwa:generate-assets && nuxi build",
		"build:sw": "SW=true nuxi build",
		"dev": "nuxt dev",
		"dev:sw": "SW=true nuxi dev",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"generate": "nuxt generate",
		"postinstall": "nuxt prepare",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prepare": "node .husky/install.mjs",
		"preview": "nuxt preview",
		"prisma:generate": "pnpm prisma generate",
		"start": "node .output/server/index.mjs",
		"typecheck": "nuxt typecheck"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.@(js|ts|vue|yml|yaml|md|json)": [
			"prettier --write"
		],
		"*.@(js|ts|vue)": [
			"eslint --fix"
		],
		"package.json": [
			"sort-package-json"
		]
	},
	"dependencies": {
		"@nuxtjs/color-mode": "catalog:build",
		"@pinia/nuxt": "catalog:build",
		"@prisma/client": "catalog:build",
		"@prisma/nuxt": "catalog:build",
		"@sapphire/ratelimits": "catalog:dev",
		"@tailwindcss/vite": "catalog:build",
		"@vee-validate/nuxt": "catalog:build",
		"@vee-validate/zod": "catalog:build",
		"@vite-pwa/nuxt": "catalog:build",
		"@vueuse/core": "catalog:frontend",
		"@vueuse/nuxt": "catalog:build",
		"@vueuse/router": "catalog:build",
		"clsx": "catalog:frontend",
		"colortranslator": "catalog:frontend",
		"consola": "catalog:frontend",
		"fuse.js": "catalog:frontend",
		"import-in-the-middle": "catalog:frontend",
		"markdown-it-anchor": "catalog:frontend",
		"nuxt": "catalog:build",
		"nuxt-authorization": "catalog:build",
		"pinia": "catalog:frontend",
		"prisma": "catalog:build",
		"prisma-json-types-generator": "catalog:build",
		"reka-ui": "catalog:build",
		"shadcn-nuxt": "catalog:build",
		"tailwind-merge": "catalog:frontend",
		"tailwind-variants": "catalog:frontend",
		"tw-animate-css": "catalog:frontend",
		"utility-types": "catalog:frontend",
		"vue-markdown-render": "catalog:frontend",
		"vue-sonner": "catalog:frontend",
		"zod": "catalog:frontend"
	},
	"devDependencies": {
		"@commitlint/cli": "catalog:dev",
		"@commitlint/config-conventional": "catalog:dev",
		"@discordjs/collection": "catalog:dev",
		"@discordjs/core": "catalog:dev",
		"@iconify-json/heroicons": "catalog:dev",
		"@iconify-json/ic": "catalog:dev",
		"@iconify-json/lucide": "catalog:dev",
		"@iconify-json/ph": "catalog:dev",
		"@nuxt/eslint": "catalog:build",
		"@nuxt/fonts": "catalog:build",
		"@nuxt/icon": "catalog:build",
		"@nuxt/image": "catalog:build",
		"@nuxt/scripts": "catalog:build",
		"@nuxtjs/seo": "catalog:build",
		"@sapphire/bitfield": "catalog:dev",
		"@sapphire/prettier-config": "catalog:dev",
		"@sapphire/result": "catalog:dev",
		"@sapphire/snowflake": "catalog:dev",
		"@sapphire/time-utilities": "catalog:dev",
		"@sapphire/utilities": "catalog:dev",
		"@sentry/nuxt": "catalog:build",
		"@tailwindcss/typography": "catalog:dev",
		"daisyui": "catalog:dev",
		"discord-api-types": "catalog:dev",
		"eslint": "catalog:dev",
		"eslint-config-prettier": "catalog:dev",
		"eslint-plugin-pnpm": "catalog:dev",
		"eslint-plugin-prettier": "catalog:dev",
		"husky": "catalog:dev",
		"lint-staged": "catalog:dev",
		"nuxt-auth-utils": "catalog:dev",
		"playwright": "catalog:dev",
		"prettier": "catalog:dev",
		"prettier-plugin-css-order": "catalog:dev",
		"prettier-plugin-tailwindcss": "catalog:dev",
		"sort-package-json": "catalog:dev",
		"tailwindcss": "catalog:dev",
		"typescript": "catalog:dev",
		"vue-tsc": "catalog:dev"
	},
	"packageManager": "pnpm@10.6.2",
	"engines": {
		"node": ">=20",
		"pnpm": ">=10"
	},
	"volta": {
		"node": "20.18.3"
	}
}

{
	"name": "@wolfstar-project/dashboard",
	"version": "1.0.0",
	"private": true,
	"license": "Apache-2.0",
	"type": "module",
	"scripts": {
		"dev": "nuxt dev",
		"dev:sw": "SW=true nuxi dev",
		"start": "node .output/server/index.mjs",
		"build": "pnpm run pwa:generate-assets && nuxi build",
		"build:sw": "SW=true nuxi build",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"typecheck": "nuxt typecheck",
		"prisma:generate": "pnpm prisma generate",
		"postinstall": "nuxt prepare",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"pwa:generate-assets": "pwa-assets-generator --preset minimal-2023 public/logo.svg"
	},
	"dependencies": {
		"@nuxtjs/color-mode": "^3.5.2",
		"@pinia/nuxt": "^0.10.1",
		"@prisma/client": "^6.3.1",
		"@prisma/nuxt": "^0.3.0",
		"@tailwindcss/vite": "^4.0.7",
		"@trpc-limiter/memory": "^1.0.0",
		"@trpc/client": "next",
		"@trpc/server": "next",
		"@vee-validate/nuxt": "^4.15.0",
		"@vee-validate/zod": "^4.15.0",
		"@vite-pwa/nuxt": "^0.10.6",
		"@vueuse/core": "12.6.1",
		"@vueuse/nuxt": "12.6.1",
		"@vueuse/router": "^12.6.1",
		"consola": "^3.4.0",
		"fuse.js": "^7.1.0",
		"markdown-it-anchor": "^9.2.0",
		"pinia": "^3.0.1",
		"pinia-plugin-persistedstate": "^4.2.0",
		"prisma": "^6.3.1",
		"prisma-json-types-generator": "^3.2.2",
		"tinycolor2": "^1.6.0",
		"trpc-nuxt": "next",
		"trpc-to-openapi": "^2.1.3",
		"ufo": "^1.5.4",
		"utility-types": "^3.11.0",
		"uuid": "^11.0.5",
		"vue-markdown-render": "^2.2.1",
		"vue-sonner": "^1.3.0",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.7.1",
		"@commitlint/config-conventional": "^19.7.1",
		"@discordjs/collection": "^2.1.1",
		"@discordjs/core": "^2.0.1",
		"@nuxt/content": "^3.1.1",
		"@nuxt/devtools": "2.0.0",
		"@nuxt/eslint": "1.0.1",
		"@nuxt/fonts": "^0.10.3",
		"@nuxt/icon": "^1.10.3",
		"@nuxt/image": "1.9.0",
		"@nuxt/scripts": "0.10.3",
		"@nuxtjs/seo": "^2.1.1",
		"@sapphire/bitfield": "^1.2.4",
		"@sapphire/prettier-config": "^2.0.0",
		"@sapphire/result": "^2.7.2",
		"@sapphire/snowflake": "^3.5.5",
		"@sapphire/time-utilities": "^1.7.14",
		"@sapphire/utilities": "^3.18.2",
		"@sentry/nuxt": "^9.1.0",
		"@tailwindcss/typography": "^0.5.16",
		"@types/tinycolor2": "^1.4.6",
		"@vite-pwa/assets-generator": "^0.2.6",
		"daisyui": "5.0.0-beta.8",
		"discord-api-types": "0.37.119",
		"eslint": "^9.20.1",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.3",
		"lint-staged": "^15.4.3",
		"nuxt": "3.15.4",
		"nuxt-og-image": "4.1.2",
		"nuxt-security": "^2.1.5",
		"playwright": "^1.50.1",
		"prettier": "^3.3.3",
		"prettier-plugin-css-order": "^2.1.2",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"sharp": "^0.33.5",
		"sharp-ico": "^0.1.5",
		"simple-git-hooks": "^2.11.1",
		"tailwindcss": "^4.0.6",
		"typescript": "^5.7.3",
		"vue-tsc": "2.1.10"
	},
	"pnpm": {
		"overrides": {
			"@trpc/client": "next",
			"@trpc/server": "next",
			"vue-tsc": "2.1.10",
			"@types/node": "20.11.5",
			"@vercel/nft": "^0.27.4"
		},
		"ignoredBuiltDependencies": [
			"sharp"
		]
	},
	"simple-git-hooks": {
		"pre-commit": "pnpm dlx lint-staged",
		"commit-msg": "pnpm commitlint --edit $1"
	},
	"lint-staged": {
		"*.@(js|ts|vue|yml|yaml|md|json)": [
			"prettier --write"
		],
		"*.@(js|ts|vue)": [
			"eslint --fix"
		]
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wolfstar-project/wolfstar.rocks.git"
	},
	"bugs": {
		"url": "https://github.com/wolfstar-project/wolfstar.rocks/issues"
	},
	"engines": {
		"node": ">=22",
		"pnpm": ">=10"
	},
	"packageManager": "pnpm@10.4.1",
	"volta": {
		"node": "22.12.0"
	}
}
